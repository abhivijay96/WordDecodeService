{
  "Hosting": {
    "Kestrel": {
      "urls": "http://*:6007"
    }
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Warning",
      "HigherKnowledge": "Information"
    }
  },

  "Caches": {
    "Redis": {
      "ConnectionString": "redis.cache.internal.higherknowledge.in:6379,abortConnect=false,connectTimeout=300"
    }
  },

  "Stores": {
    "PostgreSql": {
      "ConnectionString": "Server=postgresql.store.internal.higherknowledge.in;Database=higherknowledge;Pooling=true;MinPoolSize=1;MaxPoolSize=20;ConnectionLifeTime=15;Port=9000"
    }
  }
}
